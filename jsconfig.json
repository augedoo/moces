{
  "compilerOptions": {
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/assets/*": ["assets/*"],
      "@/components/*": ["components/*"],
      "@/styles/*": ["styles/*"],
      "@/config/*": ["config/*"],
      "@/store/*": ["store/*"],
      "@/helpers/*": ["helpers/*"],
      "@/utils/*": ["utils/*"],
      "@/pages/*": ["pages/*"]
    }
  }, 
  "exclude": ["node_modules", ".next"]
}